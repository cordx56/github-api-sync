{
  "name": "obsidian-github-api-sync",
  "version": "0.1.0",
  "description": "Obsidian plugin for syncing vault using GitHub API",
  "main": "dist/main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "fmt": "prettier -w src"
  },
  "keywords": [
    "Obsidian",
    "Plugin",
    "GitHub",
    "Sync"
  ],
  "author": "cordx56 <cordx56@cordx.cx>",
  "license": "MIT",
  "dependencies": {
    "diff": "^8.0.2",
    "gitignore-parser": "^0.0.2"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "prettier": "^3.6.2",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}
